<%= @landmark.name %>
<form action="/landmarks/<%=@landmark.id%>" method="POST">
  <input type="hidden" name="_method" value="PATCH">
    Name: <input type="text" id="name" name="landmark[name]" value=<%= @landmark.name %>>
    Year Completed: <input type="text" id="year_completed" name="landmark[year_completed]" value=<%= @landmark.year_completed %>>
    <input type="submit" value="Edit Landmark">
</form>